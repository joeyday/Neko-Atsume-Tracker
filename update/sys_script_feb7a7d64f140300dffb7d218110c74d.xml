        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	var gr = current.cat.getRefRecord();
	gr.collected_picture = true;
	if (current.pose.name == 'Butthole') gr.collected_butthole_picture = true;
	if (current.pose.name == 'Loaf') gr.collected_loaf_picture = true;
	if (current.pose.name == 'Faceplant') gr.collected_faceplant_picture = true;
	
	if (gr.collected_picture.changes() || gr.collected_butthole_picture.changes() || gr.collected_loaf_picture.changes() || gr.collected_faceplant_picture.changes()) {
		gr.update();
	}
	
})(current, previous);]]></script>
